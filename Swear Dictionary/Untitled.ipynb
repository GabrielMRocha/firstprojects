   "source": [
    "from googletrans import Translator\n",
    "\n",
    "lang = [\"pt\",\"en\",\"cs\",\"sk\",\"nl\"]\n",
    "translator = Translator()\n",
    "word = input(\"Write the swearword: \")\n",
    "for i in lang:\n",
    "    result = translator.translate(word,dest=i)\n",
    "    print(result.text)\n"
   ]
